    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.10",
    "aws-sdk": "^2.1570.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "common-tags": "^1.8.2",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
