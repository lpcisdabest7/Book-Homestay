    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "nestjs": "^0.0.1",
    "nestjs-cls": "^4.4.0",
    "npm": "^10.8.2",
    "oauth2-client": "^0.0.4",
    "parse-duration": "^1.1.0",
